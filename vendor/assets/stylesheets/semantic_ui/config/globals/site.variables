/*******************************
     User Global Variables
*******************************/

/*-------------------
    Site Colors
--------------------*/

@bingogreen : #b2e59d;
@bingodark : #32826E;

/*---  Brand Colors  ---*/

@primaryColor        : @bingogreen;
@secondaryColor      : @bingodark;

/*-------------------
      Page
--------------------*/

@pageBackground      : #F7F7F7;
@pageOverflowX       : hidden;

@lineHeight          : 1.33;
@textColor           : rgba(0, 0, 0, 0.8);

@headerMargin        : 1em 0em 1rem;
@paragraphMargin     : 0em 0em 1em;
